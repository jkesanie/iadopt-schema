@prefix ex: <http://example.org/iadopt/> .
@prefix iadopt: <https://w3id.org/iadopt/ont/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .


ex:Variable
  a           iadopt:Variable ;
  rdfs:label  "Testing Variable"@en ;
  iadopt:hasObjectOfInterest    ex:OoI ;
  iadopt:hasProperty            ex:Prop ;
  iadopt:hasStatisticalModifier ex:StatMod1, ex:StatMod2 ;
  .


ex:Prop
  a          iadopt:Property ;
  rdfs:label "property"@en ;
  .

ex:OoI
  a          iadopt:Entity ;
  rdfs:label "object of interest"@en ;
  .

ex:StatMod1
  a          iadopt:StatisticalModifier ;
  rdfs:label "statistical modifier 1"@en ;
  .

ex:StatMod2
  a          iadopt:StatisticalModifier ;
  rdfs:label "statistical modifier 2"@en ;
  .
