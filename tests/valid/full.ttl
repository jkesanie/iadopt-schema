@prefix ex: <http://example.org/iadopt/> .
@prefix iadopt: <https://w3id.org/iadopt/ont/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .


ex:Variable
  a           iadopt:Variable ;
  rdfs:label  "Testing Variable"@en ;
  iadopt:hasObjectOfInterest    ex:OoI ;
  iadopt:hasProperty            ex:Prop ;
  iadopt:hasMatrix              ex:Matrix ;
  iadopt:hasContextObject       ex:Context ;
  iadopt:hasStatisticalModifier ex:StatMod ;
  iadopt:hasConstraint [
    a           iadopt:Constraint ;
    rdfs:label  "constraint OoI"@en ;
    iadopt:constrains ex:OoI ;
  ], [
    a           iadopt:Constraint ;
    rdfs:label  "constraint Prop"@en ;
    iadopt:constrains ex:Prop ;
  ], [
    a           iadopt:Constraint ;
    rdfs:label  "constraint Matrix"@en ;
    iadopt:constrains ex:Matrix ;
  ], [
    a           iadopt:Constraint ;
    rdfs:label  "constraint Context"@en ;
    iadopt:constrains ex:Context ;
  ], [
    a           iadopt:Constraint ;
    rdfs:label  "constraint StatMod"@en ;
    iadopt:constrains ex:StatMod ;
  ]
  .


ex:Prop
  a          iadopt:Property ;
  rdfs:label "property"@en ;
  .

ex:OoI
  a          iadopt:Entity ;
  rdfs:label "object of interest"@en ;
  .

ex:Matrix
  a          iadopt:Entity ;
  rdfs:label "matrix"@en ;
  .

ex:Context
  a          iadopt:Entity ;
  rdfs:label "context"@en ;
  .

ex:StatMod
  a          iadopt:StatisticalModifier ;
  rdfs:label "statistical modifier"@en ;
  .
